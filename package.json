{
  "name": "@adjudication/types",
  "version": "1.19.1",
  "description": "The project refers to the types of Adjudication package that will be published to NPM and shared between backend and frontend for reusablity and ensuring consistency.",
  "type": "module",
  "main": "./mod.js",
  "types": "./mod.d.ts",
  "files": [
    "mod.js",
    "mod.d.ts"
  ],
  "exports": {
    ".": {
      "import": "./mod.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sociable-tech/adj-types.git"
  },
  "author": {
    "name": "Sheraz Ahmed",
    "email": "sherazahmdd@gmail.com"
  },
  "scripts": {
    "build:npm": "deno run -A scripts/build_npm.ts $(node -p \"require('./package.json').version\") && rollup -c",
    "bump:deno": "node scripts/bump_deno.js",
    "version": "npm run bump:deno && git add deno.json",
    "postversion": "npm run build:npm && git push --follow-tags"
  },
  "license": "MIT",
  "devDependencies": {
    "rollup": "^4.52.2",
    "rollup-plugin-dts": "^6.2.3",
    "standard-version": "^9.5.0",
    "typescript": "^5.7.3"
  }
}
